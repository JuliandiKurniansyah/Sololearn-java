import java.util.Scanner;

public class Program
{
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int amount = scanner.nextInt();
		//your code goes here
		int percent;
		int payment;
		int a = amount;
		for (int x = 0 ; x < 3; x++) {
			percent = a * 10 /100;
			payment = a - percent;
			a = payment;
    	}
		System.out.println(a);
		 
	}
}
